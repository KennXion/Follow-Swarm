{
  "name": "spotify-follow-swarm",
  "version": "1.0.0",
  "description": "Automated Spotify artist follow exchange platform",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "dev:api": "nodemon src/index.js",
    "dev:worker": "nodemon src/worker.js",
    "dev:frontend": "cd client && npm run dev",
    "start": "node src/index.js",
    "worker": "node src/worker.js",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": ["spotify", "music", "growth", "automation"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "spotify-web-api-node": "^5.0.2",
    "pg": "^8.11.3",
    "redis": "^4.6.8",
    "ioredis": "^5.3.2",
    "bull": "^4.11.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "joi": "^17.9.2",
    "winston": "^3.10.0",
    "morgan": "^1.10.0",
    "axios": "^1.5.0",
    "uuid": "^9.0.0",
    "crypto": "^1.0.1",
    "compression": "^1.7.4",
    "express-validator": "^7.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.4",
    "supertest": "^6.3.3",
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "@types/node": "^20.5.7",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}